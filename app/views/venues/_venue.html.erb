<div id="<%= dom_id venue %>">
  <div>
    <% if venue.cover.attached? %>
      <% if action_name != "show" %>
        <%= link_to venue do %>
          <img src="<%= url_for(venue.cover) %>" class="object-cover w-full h-48">
        <% end %>
      <% else %>
        <%= image_tag venue.cover %>
      <% end %>
    <% end %>
  </div>
  <p class="my-5">
    <strong class="block mb-1 font-medium"><%= venue.name %></strong>
  </p>
</div>
